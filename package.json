{
  "test": "jest",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mainnet-cash/mainnet-js/"
  },
  "bugs": {
    "url": "https://github.com/mainnet-cash/mainnet-js/issues"
  },
  "name": "mainnet-js",
  "version": "0.0.7",
  "homepage": "https://mainnet.cash",
  "main": "dist/mainnet.node.js",
  "types": "index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.js.map"
  ],
  "scripts": {
    "pretest": "node util/checkBchd.js",
    "test": "npx jest",
    "debug:grpcui-install": "go get github.com/fullstorydev/grpcui/... && go install github.com/fullstorydev/grpcui/cmd/grpcui",
    "debug:grpcui": "grpcui -proto pb/bchrpc.proto -insecure localhost:18335",
    "debug:pb-update": "wget https://raw.githubusercontent.com/gcash/bchd/master/bchrpc/bchrpc.proto -O pb/bchrpc.proto",
    "codegen:list": "npx openapi-generator list",
    "codegen:server": "npx openapi-generator generate -i swagger/v1/api.yml -g nodejs-express-server -o generated/serve/",
    "codegen:typescript-mock": "npx openapi-generator generate -i swagger/v1/api.yml -g typescript-node -o generated/client/typescript-mock",
    "format": "prettier --write .",
    "serve": "npx ts-node ./generated/serve/index.ts",
    "serve:monitor": "npx nodemon ./generated/serve/index.ts",
    "build": "npx webpack"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.15-4.3.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.6.0",
    "@types/swagger-ui-express": "^4.1.2",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-openapi-validator": "^3.9.1",
    "fake-indexeddb": "^3.1.2",
    "jest": "^26.3.0",
    "js-yaml": "^3.3.0",
    "nodemon": "^2.0.4",
    "ono": "^5.0.1",
    "openapi-sampler": "^1.0.0-beta.15",
    "prettier": "^2.0.5",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "unzipper": "^0.10.11",
    "wasm-loader": "^1.3.0",
    "webpack": "^v5.0.0-beta.28",
    "webpack-cli": "^3.3.12",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "@bitauth/libauth": "^1.17.1",
    "dexie": "^3.0.2",
    "grpc-bchrpc-node": "^0.10.2",
    "grpc-bchrpc-web": "^0.10.1"
  },
  "keywords": [
    "bch",
    "bitcoin cash"
  ],
  "author": "readcash"
}
